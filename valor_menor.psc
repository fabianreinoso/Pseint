Algoritmo valor_menor
	Escribir 'INGRESE EL PRIMER NUMERO'
	Leer a
	Escribir 'INGREE EL SEGUNDO NUMERO'
	Leer b
	si a = b Entonces
		Escribir a,' y ',b ,' SON IGUALES '
	SiNo
		si a < b Entonces
			Escribir a,' es el numero menor'
		SiNo
			Escribir b,' es el numero menor'
		FinSi
	FinSi
FinAlgoritmo
