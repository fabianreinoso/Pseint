Algoritmo promedio_1
	Escribir 'INGRESE EL TOTAL DE CURSOS'
	Leer n
	Escribir 'INGRESE SU NOTA DE TEORIA'
	leer t
	Escribir 'INGRESE SU NOTA DE PRACTICAS'
	Leer p
	Escribir 'INGRESE SU NOTA DE EXAMEN'
	Leer e
	promedio <- (t+p+e)
	final <- (promedio / n)
	Escribir 'SU PROMEDIO FINAL ES DE: ', final
FinAlgoritmo
